{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"module": "esnext",
		"moduleResolution": "node",
		"target": "es6",
		"jsx": "react",
		"allowJs": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"newLine": "LF",
		"noImplicitThis": true,
		"pretty": true,
		"strict": true
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules"]
}
